        beanPackage="oracle.retail.stores.pos.ui.beans"
        beanClassName="NavigationButtonBean">

        <BUTTON
            actionName="Search"
            enabled="true"
            keyName="F2"
            labelTag="Search"/>
        <BUTTON
            actionName="Add"
            enabled="true"
            keyName="F3"
            labelTag="Add"/>
        <BUTTON
            actionName="Inventory"
            enabled="true"
            keyName="F4"
