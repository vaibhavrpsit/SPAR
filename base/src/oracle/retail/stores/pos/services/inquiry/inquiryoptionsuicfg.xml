        <BUTTON
            actionName="UPSELL"
            enabled="true"
            keyName="F6"
            labelTag="UpsellRelatedItems"/>
        <BUTTON
            actionName="SUBSTITUTE"
            enabled="true"
            keyName="F7"
            labelTag="SubstitutionRelatedItems"/> 
        <BUTTON
            actionName="AUTO"
            enabled="true"
            keyName="F8"
            labelTag="AutoRelatedItems"/>  
	    <BUTTON
            actionName="WebStore"
            enabled="true"
            keyName="F2"
            labelTag="WebstoreSearch"/> 
	    <BUTTON
            actionName="Inventory"
            enabled="true"
            keyName="F3"
            labelTag="ItemInquiry"/>
    </BEAN>

    <BEAN
        specName="ItemDisplayButtonSpec"
        configuratorPackage="oracle.retail.stores.pos.ui"
