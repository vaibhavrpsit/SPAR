        beanClassName="SaleLocalButtonBean">

        <BUTTON
            actionName="Add"
            enabled="true"
            keyName="F2"
            labelTag="Add"/>
        <BUTTON
            actionName="Detail"
            enabled="true"
            keyName="F3"
            labelTag="Detail"/>
        <BUTTON
            actionName="FilterResults"
