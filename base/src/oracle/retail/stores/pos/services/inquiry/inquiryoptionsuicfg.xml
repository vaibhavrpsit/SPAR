        <BUTTON
            actionName="Undo"
            keyName="Esc"
            enabled="false"
            labelTag="Undo"/>

        <BUTTON
            actionName="Next"
            keyName="Enter"
            enabled="true"
            labelTag="Next"/>

        <BUTTON
            actionName="Keys"
            keyName="Ctrl+K"
            enabled="true"
            labelTag="Keys"
            label="Keys"
            parameterName="OnScreenKeyboardEnabled"
            actionListenerName="oracle.retail.stores.pos.ui.behavior.KeyDialogAction"/>
