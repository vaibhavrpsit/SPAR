<!DOCTYPE UICFG SYSTEM "classpath://oracle/retail/stores/pos/config/defaults/posui.dtd">

<UICFG>
    <!-- Beans Inquiry Options Main screen -->
    <BEAN
        specName="InquiryOptionsSpec"
        configuratorPackage="oracle.retail.stores.pos.ui"
        configuratorClassName="POSBeanConfigurator"
        beanPackage="oracle.retail.stores.pos.ui.beans"
        beanClassName="EmptyBean">
    </BEAN>
    <BEAN
        specName="InquiryOptionsButtonsSpec"
        configuratorPackage="oracle.retail.stores.pos.ui"
        configuratorClassName="POSBeanConfigurator"
        beanPackage="oracle.retail.stores.pos.ui.beans"
        beanClassName="NavigationButtonBean">

        <BUTTON
            actionName="Price"
            enabled="true"
            keyName="F2"
            labelTag="Price"/>

        <BUTTON
            actionName="GiftCard"
            enabled="true"
            keyName="F3"
            labelTag="GiftCard"/>
        <BUTTON
            actionName="GiftCode"
            enabled="true"
            keyName="F4"
            labelTag="GiftCode"/>
    </BEAN>

    <!-- Beans for Item Inquiry Service -->
    <BEAN
        specName="ItemInquirySpec"
        configuratorPackage="oracle.retail.stores.pos.ui"
        configuratorClassName="POSBeanConfigurator"
        beanPackage="oracle.retail.stores.pos.ui.beans"
        beanClassName="ItemInquiryBean">
    </BEAN>

    <BEAN
        specName="ItemDisplaySpec"
        configuratorPackage="oracle.retail.stores.pos.ui"
        configuratorClassName="POSBeanConfigurator"
        beanPackage="oracle.retail.stores.pos.ui.beans"
        beanClassName="ItemInfoTabbedUIBean">
        <BEANPROPERTY propName="recommendedItemsBean" propValue="oracle.retail.stores.pos.ui.beans.ExtendedItemDataListBean"/>
        <BEANPROPERTY propName="recommendedLabelTags" propValue="ItemHeaderLabel,DepartmentHeaderLabel,PriceHeaderLabel"/>
        <BEANPROPERTY propName="recommendedLabelWeights" propValue="30,40,30"/>
        <BUTTON
            actionName="Summary"
            enabled="true"
            keyName="S"
            labelTag="Summary"/>
        <BUTTON
            actionName="Detail"
            enabled="true"
            keyName="D"
            labelTag="Detail"/>
        <BUTTON
            actionName="RecommendedItems"
            enabled="true"
            keyName="R"
            labelTag="RecommendedItems"/>
    </BEAN>

    <BEAN
        specName="EditableItemDisplaySpec"
        configuratorPackage="oracle.retail.stores.pos.ui"
        configuratorClassName="POSBeanConfigurator"
        beanPackage="oracle.retail.stores.pos.ui.beans"
        beanClassName="EditableItemInfoBean">
        <BEANPROPERTY propName="maxFieldLengths" propValue="14,32,9"/>
    </BEAN>
    
    <BEAN
        specName="FilterItemSearchResultsSpec"
        configuratorPackage="oracle.retail.stores.pos.ui"
        configuratorClassName="POSBeanConfigurator"
        beanPackage="oracle.retail.stores.pos.ui.beans"
        beanClassName="FilterItemSearchResultsBean">
        <BEANPROPERTY propName="maxFieldLengths" propValue="14,32,9"/>
    </BEAN>
    
    <BEAN
        specName="ItemListSpec"
