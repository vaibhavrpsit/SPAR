<!DOCTYPE SERVICE SYSTEM "classpath://oracle/retail/stores/foundation/tour/dtd/tourguide.dtd">

<SERVICE
    name="returnfindtransaction"
    package="oracle.retail.stores.pos.services.returns.returnfindtrans"
    tourcam="OFF">
    <COMMENT >
        This services searches for transactions by Transaction
        ID and allows the user select a transaction if the
        application retrieves more than one.
    </COMMENT>
    <CARGO  class="ReturnFindTransCargo"/>
    <SERVICECODE >
        <LANEACTION class="CheckDatabaseErrorAisle"/>
        <LANEACTION class="DataBaseErrorAisle" package="oracle.retail.stores.pos.services.common"/>
        <LANEACTION class="FailureConversionAisle" package="oracle.retail.stores.pos.services.common"/>
        <LANEACTION class="GetSelectedTransactionRoad"/>
        <LANEACTION class="GetTransactionSummaryIndexRoad"/>
        <LANEACTION class="InvalidConversionAisle" package="oracle.retail.stores.pos.services.common"/>
        <LANEACTION class="MoveTransactionToOriginalRoad"/>
        <LANEACTION class="ReturnItemConversionAisle" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <LANEACTION class="RollbackTransactionSummaryRoad"/>
        <LANEACTION class="TooManyTransactionsConversionAisle" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <LANEACTION class="TooManyTransactionsErrorAisle" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <LANEACTION class="TransReentryRoad"/>
        <LANEACTION class="UndoCustomerRoad" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <LANEACTION class="ValidateTransactionIDAisle" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        
        <LETTER name="Cancel"/>
        <LETTER name="Continue"/>
        <LETTER name="DbError"/>
        <LETTER name="Failure"/>
        <LETTER name="Failure2"/>
        <LETTER name="Invalid"/>
        <LETTER name="Loop"/>
        <LETTER name="MultipleMatches"/>
        <LETTER name="Next"/>
        <LETTER name="NotCBRReturn" />
        <LETTER name="Ok"/>
        <LETTER name="Receipt"/>
        <LETTER name="Retrieve"/>
        <LETTER name="ReturnItem"/>
        <LETTER name="Success"/>
        <LETTER name="Tender"/>
        <LETTER name="TooMany"/>
        <LETTER name="TooManyTransactions"/>
        <LETTER name="TransactionHasOrder"/>
        <LETTER name="TransactionID"/>
        <LETTER name="TransactionNotFound"/>
        <LETTER name="Undo"/>
        <LETTER name="crossborderreturn"/>
        
        <SHUTTLE class="CBRLaunchShuttle" package="oracle.retail.stores.pos.services.returns.returnfindtrans.crossborderreturn"/>
        <SHUTTLE class="CBRReturnShuttle" package="oracle.retail.stores.pos.services.returns.returnfindtrans.crossborderreturn"/>
        <SHUTTLE class="ReturnTransactionLaunchShuttle"/>
        <SHUTTLE class="ReturnTransactionReturnShuttle"/>
        
        <SIGNAL  class="IsDBErrorSignal"/>
        <SIGNAL  class="IsNotDBErrorSignal" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <SIGNAL  class="IsThereMoreThanOneTransactionSignal"/>
        <SIGNAL  class="IsThereOneTransactionSignal"/>
        <SIGNAL  class="IsWebManagedOrderSignal" package="oracle.retail.stores.pos.services.order.common"/>
        <SIGNAL  class="TransReentryLight"/>
        
        <SITEACTION class="CBRCheckSite"/>
        <SITEACTION class="CheckForOrderIDSite" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <SITEACTION class="CheckForReturnableItemSite" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <SITEACTION class="EnterReceiptIDSite"/>        
        <SITEACTION class="EvaluateSearchCriteria"/>
        <SITEACTION class="EvaluateSearchCriteriaSite"/>
        <SITEACTION class="LookupReturnOrderSite" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <SITEACTION class="ReadSelectedTenderTransactionSite"/>
        <SITEACTION class="ReadTransactionSite"/>
        <SITEACTION class="SelectTransactionSite" package="oracle.retail.stores.pos.services.returns.returncommon"/>
        <SITEACTION class="SelectTransactionSummarySite"/>

    </SERVICECODE>
    <MAP >
        <REGION  region="SERVICE" startsite="EvaluateSearchCriteria">

            <SITE  name="EvaluateSearchCriteria" siteaction="EvaluateSearchCriteriaSite">
                <ROAD
                    name="SearchByReceiptSelected"
                    letter="Receipt"
                    destination="EnterReceiptID"
                    tape="ADVANCE"
                    record="OFF"
                    index="ON">
                </ROAD>
                <ROAD
                    name="SearchByTenderSelected"
                    letter="Tender"
                    destination="SearchTransactionByTender"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="SearchByTransactionID"
                    letter="TransactionID"
                    destination="ReadTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
            </SITE>
            
<!-- Start search by Gift Receipt -->			
            <SITE  name="EnterReceiptID" siteaction="EnterReceiptIDSite">
                <AISLE
                    name="NextEnterReceiptID"
                    letter="Next"
                    laneaction="ValidateTransactionIDAisle">
                </AISLE>
                <ROAD
                    name="OkEnterReceiptID"
                    letter="Ok"
                    destination="EnterReceiptID"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="ContinueEnterReceiptID"
                    letter="Continue"
                    destination="ReadTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <LIGHT  signal="TransReentryLight" negate="Y"/>
                </ROAD>
                <ROAD
                    name="ReturnItemReadTransactions"
                    laneaction="TransReentryRoad"
                    letter="Continue"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <LIGHT  signal="TransReentryLight"/>
                </ROAD>
                <ROAD
                    name="UndoEnterReceiptID"
                    letter="Undo"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="CancelEnterReceiptID"
                    letter="Cancel"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
            </SITE>
<!-- End search by Gift Receipt -->	

<!-- Start search by transaction ID -->
            <SITE  name="ReadTransaction" siteaction="ReadTransactionSite">
                <AISLE
                    name="DbErrorReadTransaction"
                    letter="DbError"
                    laneaction="DataBaseErrorAisle">
                </AISLE>
                <AISLE
                    name="TooManyReadTransaction"
                    letter="TooMany"
                    laneaction="TooManyTransactionsErrorAisle">
                </AISLE>
                <AISLE
                    name="OkReadTransactionDBError"
                    letter="Ok"
                    laneaction="CheckDatabaseErrorAisle">
                    <LIGHT  signal="IsDBErrorSignal"/>
                </AISLE>
                <ROAD 
                    name="ReturnItemReadTransactions"
                    letter="Ok"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <LIGHT  signal="IsDBErrorSignal" negate="Y"/>
                </ROAD>
                <ROAD
                    name="RetryItemSearchTransaction"
                    letter="Retry"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="SuccessReadTransaction"
                    letter="Success"
                    destination="CheckForReturnableItem"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="SuccessReadTransactions"
                    letter="MultipleMatches"
                    destination="SelectTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="SuccessTransactionWithWebOrder"
                    letter="TransactionHasOrder"
                    laneaction="MoveTransactionToOriginalRoad"
                    destination="CheckForReturnableItem"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <LIGHT signal="IsWebManagedOrderSignal"/>
                </ROAD>
                <ROAD
                    name="SuccessReadTransactionWithOrder"
                    letter="TransactionHasOrder"
                    destination="LookupOrderFromTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <LIGHT signal="IsWebManagedOrderSignal" negate="Y"/>
                </ROAD>
                <ROAD
                    name="ReturnItemReadTransactions"
                    letter="TransactionNotFound"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="InvalidReadTransaction"
                    letter="Invalid"
                    destination="Final"
                    tape="ADVANCE"
                    record="ON"
                    index="ON">
                    <COMMENT >
                        Transaction read is invalid for return such as house
                        account payment transaction.
                    </COMMENT>
                </ROAD>
            </SITE>

            <SITE  name="SelectTransaction" siteaction="SelectTransactionSite">
                <ROAD
                    name="AcceptSelectTransaction"
                    letter="Next"
                    laneaction="GetSelectedTransactionRoad"
                    destination="CheckForOrderID"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="UndoSelectTransaction"
                    letter="Undo"
                    laneaction="UndoCustomerRoad"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="CancelSelectTransaction"
                    letter="Cancel"
                    laneaction="UndoCustomerRoad"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
            </SITE>

            <SITE name="CheckForOrderID" siteaction="CheckForOrderIDSite">
                <ROAD
                    name="LookupOrder"
                    letter="TransactionHasOrder"
                    destination="LookupOrderFromTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="ContinueWithTransaction"
                    letter="Continue"
                    destination="CheckForReturnableItem"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
            </SITE>

            <SITE name="LookupOrderFromTransaction" siteaction="LookupReturnOrderSite">
                <AISLE 
                    name="LookupOrderOkFailure" 
                    letter="Ok" 
                    laneaction="FailureConversionAisle">
                </AISLE>
                <AISLE 
                    name="LookupOrderDBError" 
                    letter="DbError" 
                    laneaction="DataBaseErrorAisle">
                </AISLE>
                <ROAD 
                    name="LookupOrderFailure" 
                    letter="Failure" 
                    destination="Final" 
                    record="OFF" 
                    index="OFF">
                </ROAD>
                <ROAD 
                    name="LookupOrderSuccess" 
                    letter="Success" 
                    destination="CheckForReturnableItem" 
                    record="OFF" index="OFF">
                </ROAD>
                <ROAD
                    name="RetryItemSearchTransaction"
                    letter="Retry"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="ReturnItemReadTransactions"
                    letter="TransactionNotFound"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
            </SITE>
            
            <SITE  name="CheckForReturnableItem" siteaction="CheckForReturnableItemSite">
                <AISLE
                    name="OkNoReturnableItemsError"
                    letter="Ok"
                    laneaction="FailureConversionAisle">
                </AISLE>
                <AISLE
                    name="Failure2Conversion"
                    letter="Failure2"
                    laneaction="FailureConversionAisle">
                </AISLE>
                <ROAD
                    name="SuccessCheckForReturnableItem"
                    letter="Success"
                    destination="CBRCheck"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="FailureCheckForReturnableItem"
                    letter="Failure"
                    laneaction="UndoCustomerRoad"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
            </SITE>
<!-- End search by transaction ID -->

<!-- Start search by tender ID -->
            <SITE  name="SearchTransactionByTender" siteaction="ReadTransactionSite">
                <AISLE
                    name="DbErrorSearchTransactionByTender"
                    letter="DbError"
                    laneaction="DataBaseErrorAisle">
                </AISLE>
                <AISLE
                    name="TooManyReadTransactions"
                    letter="TooMany"
                    laneaction="TooManyTransactionsErrorAisle">
                </AISLE>
                <AISLE
                    name="OkReadTransactionsTooMany"
                    letter="Ok"
                    laneaction="TooManyTransactionsConversionAisle">
                </AISLE>
                <ROAD
                    name="SuccessSearchTransactionByTender"
                    letter="Success"
                    destination="ReadTenderItemTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="SuccessSearchTransactionWithOrder"
                    letter="TransactionHasOrder"
                    destination="LookupOrderFromTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="SuccessSearchTransactionsByTender"
                    letter="MultipleMatches"
                    destination="SelectTenderTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="RetryItemSearchTransactionByTender"
                    letter="Retry"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="ReturnItemSearchTransactionByTender"
                    letter="TransactionNotFound"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="InvalidSearchTransactionByTender"
                    letter="Invalid"
                    destination="Final"
                    tape="ADVANCE"
                    record="ON"
                    index="ON">
                    <COMMENT >
                        Transaction read is invalid for return such as house
                        account payment transaction.
                    </COMMENT>
                </ROAD>
                <ROAD
                    name="TooManyTransactionsAcknowledged"
                    letter="TooManyTransactions"
                    destination="Final"
                    tape="ADVANCE"
                    record="ON"
                    index="ON">
                    <COMMENT >
                        Too many transactions were returned.
                    </COMMENT>
                </ROAD>
            </SITE>
            
            <SITE  name="SelectTenderTransaction" siteaction="SelectTransactionSummarySite">
                <ROAD
                    name="AcceptTenderTransaction"
                    letter="Next"
                    laneaction="GetTransactionSummaryIndexRoad"
                    destination="CheckForOrderIDTender"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <COMMENT >
                        Goes to a site to retrieve the items for the selected
                        transaction for the provided tender and item
                    </COMMENT>
                </ROAD>
                <ROAD
                    name="CancelSelectTenderTransaction"
                    letter="Cancel"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="UndoSelectTenderTransaction"
                    letter="Undo"
                    laneaction="RollbackTransactionSummaryRoad"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <COMMENT >
                        Returns to site to allow the re-entry of the item number.
                    </COMMENT>
                </ROAD>
            </SITE>

            <SITE name="CheckForOrderIDTender" siteaction="CheckForOrderIDSite">
                <ROAD
                    name="LookupOrder"
                    letter="TransactionHasOrder"
                    destination="LookupOrderFromTransactionTender"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="ContinueWithTransaction"
                    letter="Continue"
                    destination="ReadTenderItemTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
            </SITE>

            <SITE name="LookupOrderFromTransactionTender" siteaction="LookupReturnOrderSite">
                <AISLE 
                    name="LookupOrderOkFailure" 
                    letter="Ok" 
                    laneaction="FailureConversionAisle">
                </AISLE>
                <AISLE 
                    name="LookupOrderDBError" 
                    letter="DbError" 
                    laneaction="DataBaseErrorAisle">
                </AISLE>
                <ROAD 
                    name="LookupOrderFailure" 
                    letter="Failure" 
                    destination="Final" 
                    record="OFF" 
                    index="OFF">
                </ROAD>
                <ROAD 
                    name="LookupOrderSuccess" 
                    letter="Success" 
                    destination="TenderReturnableItems" 
                    record="OFF" index="OFF">
                </ROAD>
            </SITE>
            
            <SITE  name="ReadTenderItemTransaction" siteaction="ReadSelectedTenderTransactionSite">
                <AISLE
                    name="DBErrorReadSelectedTransaction"
                    letter="DbError"
                    laneaction="DataBaseErrorAisle">
                </AISLE>
                <AISLE
                    name="OkReadSelectedTransaction"
                    letter="Ok"
                    laneaction="FailureConversionAisle">
                </AISLE>
                <ROAD
                    name="SuccessReadSelectedTransaction"
                    letter="Success"
                    destination="TenderReturnableItems"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="FailureReadSelectedTransaction"
                    letter="Failure"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="SuccessTransactionWithWebOrder"
                    letter="TransactionHasOrder"
                    laneaction="MoveTransactionToOriginalRoad"
                    destination="TenderReturnableItems"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <LIGHT signal="IsWebManagedOrderSignal"/>
                </ROAD>
                <ROAD
                    name="SuccessReadTransactionWithOrder"
                    letter="TransactionHasOrder"
                    destination="LookupOrderFromTransaction"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                    <LIGHT signal="IsWebManagedOrderSignal" negate="Y"/>
                </ROAD>
            </SITE>
            
            <SITE  name="TenderReturnableItems" siteaction="CheckForReturnableItemSite">
                <AISLE
                    name="OkNoTenderReturnableItems"
                    letter="Ok"
                    laneaction="InvalidConversionAisle">
                </AISLE>
                <AISLE
                    name="Failure2Conversion"
                    letter="Failure2"
                    laneaction="FailureConversionAisle">
                </AISLE>
                <ROAD
                    name="SuccessTenderReturnableItems"
                    letter="Success"
                    destination="ReturnTransactionStation"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="FailureTenderReturnableItems"
                    letter="Failure"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="InvalidTenderReturnableItems"
                    letter="Invalid"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
            </SITE>
<!-- End search by tender ID -->
            
<!-- Start cross border return -->
            <SITE  name="CBRCheck" siteaction="CBRCheckSite">
            	 <ROAD
            	    name="CBRSuccess"
            	    letter="crossborderreturn"
            	    destination="CrossBorderReturnStation"	
            	    tape="ADVANCE"
             		record="OFF"
             		index="OFF">
             	</ROAD>
             	<ROAD
             		name="CBRFailure"
             		letter="Failure"
             		destination="Final"
             		tape="ADVANCE"
             		record="OFF"
             		index="OFF">
             	</ROAD>
             	<ROAD
             		name="NotCBRReturn"
             		letter="NotCBRReturn"
             		destination="ReturnTransactionStation"
             		tape="ADVANCE"
             		record="OFF"
             		index="OFF">
             	</ROAD>
             	
            </SITE>
            
            <STATION
                name="CrossBorderReturnStation"
                servicename="returns.returnsfindtrans.crossborderreturn"
                targettier="CURRENTTIER"
                launchshuttle="CBRLaunchShuttle"
                returnshuttle="CBRReturnShuttle">
                <ROAD
                    name="ReturnSuccess"
                    letter="Success"
                    destination="ReturnTransactionStation"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                
             </STATION>
<!-- End cross border return -->
             
            <STATION
                name="ReturnTransactionStation"
                servicename="returns.returntransaction.returntransaction"
                targettier="CURRENTTIER"
                launchshuttle="ReturnTransactionLaunchShuttle"
                returnshuttle="ReturnTransactionReturnShuttle">
                <ROAD
                    name="UndoTransactionStationOne"
                    letter="Undo"
                    laneaction="UndoCustomerRoad"
                    destination="Final"
                    tape="DISCARD"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="SuccessReturnItem"
                    letter="Success"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="FailureReturnItem"
                    letter="Failure"
                    laneaction="UndoCustomerRoad"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
                    index="OFF">
                </ROAD>
                <ROAD
                    name="CancelReturnItem"
                    letter="Cancel"
                    laneaction="UndoCustomerRoad"
                    destination="Final"
                    tape="ADVANCE"
                    record="OFF"
