<!DOCTYPE SERVICE SYSTEM "classpath://oracle/retail/stores/foundation/tour/dtd/tourguide.dtd">

<SERVICE name="buildfftour"
         package="oracle.retail.stores.pos.services.buildfftour">
    
    <CARGO class="BuildFFTourCargo"/>
    
    <SERVICECODE>

        <LETTER name="Next"/>

        <SITEACTION class="BuildFileSite"/>

    </SERVICECODE>

    <MAP>
    
        <REGION region="SERVICE" 
                startsite="BuildFile">
                
            <SITE name="BuildFile" siteaction="BuildFileSite">
                <ROAD name="BuildFileNext"
                      letter="Next"
                      destination="Final">
                </ROAD>
            </SITE>

            <SYSTEMSITE name="Final" action="RETURN"/>
        
        </REGION>
    
    </MAP>

</SERVICE>
