<!DOCTYPE JDBCEXCEPTIONMAPPING SYSTEM "classpath://oracle/retail/stores/foundation/tour/dtd/jdbcexceptionmapping.dtd">

<JDBCEXCEPTIONMAPPING>
    <COMMENT>
              MySQL specific exceptions.
    </COMMENT>

    <ADDMAPPING
        sqlstate="08S01"
        description="Error generated when MySQL: fails to connect"
        crsexception="CONNECTION_ERROR"
    />
    <!-- Adding Lock wait timeout as Connection Error to avoid loosing transaction -->
    <ADDMAPPING
        sqlstate="S1000"
        description="Lock wait timeout exceeded; Try restarting transaction"
        crsexception="CONNECTION_ERROR"
    />

    <ADDMAPPING
        sqlstate="42S02"
        description="Error generated from MySQL: Correlation name 'name' not found"
        crsexception="SQL_ERROR"
    />
    <ADDMAPPING
        sqlstate="23000"
        description="Error generated from MySQL: Duplicate entry"
        crsexception="REFERENTIAL_INTEGRITY_ERROR"
    />

    <ADDMAPPING
        sqlstate="42000"
        description="The data types of the operands for the operation '=' are not compatible."
        crsexception="SQL_ERROR"
    />

    <ADDMAPPING
        sqlstate="HY000"
        description="An unexpected token was found."
        crsexception="SQL_ERROR"
    />
    <ADDMAPPING
        sqlstate="S1009"
        description="Error generated from MySQL: Duplicate entry"
        crsexception="REFERENTIAL_INTEGRITY_ERROR"
    />
</JDBCEXCEPTIONMAPPING>

